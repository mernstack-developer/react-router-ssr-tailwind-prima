  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@react-router/node": "^7.5.3",
    "@remix-run/router": "^1.23.0",
    "isbot": "^5.1.27",
    "prisma": "^6.17.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "tailwindcss": "^4.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "prisma": {
    "seed": "ts-node --esm prisma/seed.ts"
